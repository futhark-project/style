{
    "CSS": {
        "src": "assets/sass/**/*.{sass,scss}",
        "dest": "assets/css"
    },
    "DOC": {
        "sassDocOptions" : {
            "dest": "docs/sass"
        }
    },
    "ERROR": "Error: <%= error.message %>",
    "PATH": {
        "src": "src/",
        "dest": "dist/"
    }
}
